{
  "name": "wcmp",
  "version": "1.2.0",
  "description": "A utility wrapper for handling sass and babel for the web.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --delay 2 --exec babel-node index.js --development --force-babel",
    "start": "babel-node index.js",
    "build": "babel-node index.js --build",
    "scripts": "babel-node index.js --scripts",
    "styles": "babel-node index.js --styles",
    "html": "babel-node index.js --html",
    "serve": "babel-node index.js --serve",
    "compile": "rimraf tooling/build && babel tooling --out-dir tooling/build",
    "test": "node tests"
  },
  "author": "Koen Houtman",
  "license": "MIT",
  "dependencies": {
    "any-json": "3.1.0",
    "ascii-art": "^1.4.3",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-custom-element-classes": "0.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "6.26.0",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "chalk": "2.3.0",
    "chokidar": "1.7.0",
    "deep-extend": "0.6.0",
    "e": "0.0.4",
    "exorcist": "1.0.0",
    "jquery": "^3.3.1",
    "json-to-sass-map": "1.0.1",
    "minimist": "1.2.0",
    "mustache": "2.3.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.5",
    "normalize-scss": "^7.0.1",
    "pretty": "2.0.0",
    "rimraf": "2.6.2",
    "roboto-fontface": "^0.9.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "bourbon": "^5.0.1",
    "express": "^4.16.3",
    "glob-fs": "^0.1.7",
    "handlebars": "^4.0.11",
    "moment": "^2.20.1",
    "node-notifier": "^5.2.1"
  }
}
